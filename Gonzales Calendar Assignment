
#include <iostream>

using namespace std;

int main()

{
	int month;
	int day;
	int year;
	cout << "Enter a Month number: ";
	cin >> month;
	cout << "Enter a Day number: ";
	cin >> day;
	cout << "Enter a Year number: ";
	cin >> year;
	
	switch(month)
	{
	case 1:
		if (day > 0 && day <= 31)
		cout << "January " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 2:
		if (day > 0 && day <= 28)
		cout << "February " << day << "," << year ;
		else if (day > 28)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 3:
		if (day > 0 && day <= 31)
		cout << "March " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 4:
		if (day > 0 && day <= 30)
		cout << "April " << day << "," << year ;
		else if (day > 30)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
	
	case 5:
		if (day > 0 && day <= 31)
		cout << "May " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
	
	case 6:
		if (day > 0 && day <= 30)
		cout << "June " << day << "," << year ;
		else if (day > 30)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
	
	case 7:
		if (day > 0 && day <= 31)
		cout << "July " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 8:
		if (day > 0 && day <= 31)
		cout << "August " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 9:
		if (day > 0 && day <= 30)
		cout << "September " << day << "," << year ;
		else if (day > 30)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
	
	case 10:
		if (day > 0 && day <= 31)
		cout << "Ocotber " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
	case 11:
		if (day > 0 && day <= 30)
		cout << "November " << day << "," << year ;
		else if (day > 30)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
	
	case 12:
		if (day > 0 && day <= 31)
		cout << "December " << day << "," << year ;
		else if (day > 31)
		cout << "Invalid Date!!";
		else
		cout << "Invalid Date!!";
		break;
		
		default:
			cout << "Invalid Date!!";
			
		}	
	return 0;
}
